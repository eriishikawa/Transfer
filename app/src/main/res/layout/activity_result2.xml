<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ResultActivity">


    <!--select-->
    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:background="@color/white"
        android:layout_marginRight="0dp"
        android:layout_marginLeft="0dp"
        android:layout_marginBottom="0dp"
        android:padding="0dp"
        android:id="@+id/linearLayout">
        <TextView
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_height="match_parent"
            android:layout_margin="0dp"
            android:gravity="center"
            android:text="ルート１"
            android:textSize="17dp"
            android:textColor="@color/white"
            android:background="@color/pink_5"
            android:id="@+id/root1" />
        <TextView
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_height="match_parent"
            android:layout_margin="0dp"
            android:gravity="center"
            android:text="ルート２"
            android:textSize="17dp"
            android:textColor="@color/gray_3"
            android:background="@color/white"
            android:id="@+id/root2" />
        <TextView
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_height="match_parent"
            android:layout_margin="0dp"
            android:gravity="center"
            android:text="ルート３"
            android:textSize="17dp"
            android:textColor="@color/gray_3"
            android:background="@color/white"
            android:id="@+id/root3" />
        <TextView
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_height="match_parent"
            android:layout_margin="0dp"
            android:gravity="center"
            android:text="ルート４"
            android:textSize="17dp"
            android:textColor="@color/gray_3"
            android:background="@color/white"
            android:id="@+id/root4" />

    </LinearLayout>

    <LinearLayout
        android:orientation="horizontal"
        android:paddingBottom="@dimen/activity_vertical_margin"
        android:layout_width="match_parent"
        android:layout_height="55dp"
        android:paddingTop="55dp"
        android:layout_marginRight="0dp"
        android:layout_marginLeft="0dp"
        android:layout_marginBottom="0dp"
        android:layout_marginTop="0dp"
        android:padding="0dp"
        android:layout_below="@+id/linearLayout"
        android:background="@color/pink_5"
        android:weightSum="1">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_marginLeft="20dp"
            android:gravity="center"
            android:text="8:18→8:25"
            android:textSize="17dp"
            android:textColor="@color/white"
            android:id="@+id/times" />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_margin="0dp"
            android:gravity="center"
            android:text="(7分)"
            android:textSize="17dp"
            android:textColor="@color/white"
            android:id="@+id/course" />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_margin="0dp"
            android:text="154円"
            android:textSize="17dp"
            android:gravity="left|center"
            android:paddingLeft="10dp"
            android:id="@+id/money" />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_marginRight="20dp"
            android:text="乗り換なし"
            android:textSize="17dp"
            android:gravity="left|center"
            android:paddingLeft="10dp"
            android:id="@+id/change" />
    </LinearLayout>


    <LinearLayout
        android:paddingLeft="@dimen/activity_horizontal_margin"
        android:paddingRight="@dimen/activity_horizontal_margin"
        android:paddingBottom="@dimen/activity_vertical_margin"
        android:paddingTop="130dp"
        android:layout_width="match_parent"
        android:layout_height="fill_parent"
        android:orientation="vertical"
        android:layout_margin="0dp"
        android:weightSum="1"
        android:id="@+id/all"
        android:focusable="false">

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@color/white"
            android:layout_marginRight="0dp"
            android:layout_marginLeft="0dp"
            android:layout_marginBottom="0dp"
            android:layout_marginTop="0dp"
            android:padding="0dp"
            android:weightSum="1">
            <LinearLayout
                android:orientation="vertical"
                android:layout_width="70dp"
                android:layout_height="match_parent"
                android:layout_margin="0dp"
                android:gravity="center"
                android:background="@color/pink_2">
                　　<TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="発"
                android:textSize="17dp"
                android:paddingTop="5dp"
                android:textColor="@color/white"
                android:id="@+id/leave" />
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:text="8:18"
                    android:textSize="17dp"
                    android:paddingTop="0dp"
                    android:textColor="@color/white"
                    android:id="@+id/leaveTime" />
            </LinearLayout>

            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_margin="0dp"
                android:gravity="center"
                android:background="@color/white">
                <TextView
                    android:layout_width="110dp"
                    android:layout_height="match_parent"
                    android:text="高田馬場駅"
                    android:textSize="17dp"
                    android:gravity="left|center"
                    android:paddingLeft="10dp"
                    android:paddingRight="10dp"
                    android:background="@color/white"
                    android:id="@+id/textSta"
                    android:layout_weight="0.05" />

                <ImageView
                    android:layout_width="45dp"
                    android:layout_height="50dp"
                    android:src="@drawable/elev"
                    android:gravity="left|center"
                    android:paddingBottom="5dp"
                    />
                <LinearLayout
                    android:orientation="vertical"
                    android:layout_width="100dp"
                    android:layout_height="match_parent">
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="早稲田口"
                        android:textSize="12dp"
                        android:paddingTop="5dp"
                        android:id="@+id/place1_1" />
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:text=""
                        android:textSize="12dp"
                        android:paddingTop="0dp"
                        />
                </LinearLayout>
            </LinearLayout>

        </LinearLayout>





        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="30dp"
            android:layout_marginRight="0dp"
            android:layout_marginLeft="0dp"
            android:layout_marginBottom="0dp"
            android:layout_marginTop="1dp"
            android:paddingTop="5dp">
            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="wrap_content"
                android:layout_height="20dp"
                android:layout_marginRight="0dp"
                android:layout_marginLeft="0dp"
                android:layout_marginBottom="0dp"
                android:layout_marginTop="0dp"
                android:padding="0dp">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_margin="0dp"
                    android:text="山手線内回り"
                    android:textSize="14dp"
                    android:gravity="left|center"
                    android:paddingLeft="10dp"
                    android:id="@+id/textWay_1" />
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_margin="0dp"
                    android:text="/新宿・渋谷方面"
                    android:textSize="14dp"
                    android:gravity="left|center"
                    android:paddingLeft="0dp"
                    android:id="@+id/textDire_1" />
            </LinearLayout>
            <LinearLayout
                android:orientation="vertical"
                android:layout_width="wrap_content"
                android:layout_height="20dp"
                android:layout_marginRight="0dp"
                android:layout_marginLeft="0dp"
                android:layout_marginBottom="0dp"
                android:layout_marginTop="0dp"
                android:padding="0dp">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_margin="0dp"
                    android:text="3駅"
                    android:textSize="14dp"
                    android:gravity="left|center"
                    android:paddingLeft="20dp"
                    android:id="@+id/textCou_1" />
            </LinearLayout>
            <LinearLayout
                android:orientation="vertical"
                android:layout_width="wrap_content"
                android:layout_height="20dp"
                android:layout_marginRight="0dp"
                android:layout_marginLeft="0dp"
                android:layout_marginBottom="0dp"
                android:layout_marginTop="0dp"
                android:padding="0dp">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_margin="0dp"
                    android:text="174円"
                    android:textSize="14dp"
                    android:gravity="left|center"
                    android:paddingLeft="20dp"
                    android:id="@+id/textMoney_22" />
            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:background="@color/white"
            android:layout_marginRight="0dp"
            android:layout_marginLeft="0dp"
            android:layout_marginBottom="0dp"
            android:layout_marginTop="0dp"
            android:padding="0dp"
            android:layout_weight="0.35">
            <LinearLayout
                android:orientation="vertical"
                android:layout_width="70dp"
                android:layout_height="match_parent"
                android:layout_margin="0dp"
                android:gravity="center"
                android:background="@color/pink_2">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="着"
                    android:textSize="17dp"
                    android:paddingTop="5dp"
                    android:textColor="@color/white"
                    android:id="@+id/arrive2" />
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:text="8:25"
                    android:textSize="17dp"
                    android:paddingTop="0dp"
                    android:textColor="@color/white"
                    android:id="@+id/arriveTime2" />
            </LinearLayout>

            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_margin="0dp"
                android:gravity="center"
                android:background="@color/white">
                <TextView
                    android:layout_width="110dp"
                    android:layout_height="match_parent"
                    android:text="新宿駅"
                    android:textSize="17dp"
                    android:gravity="left|center"
                    android:paddingLeft="10dp"
                    android:paddingRight="10dp"
                    android:background="@color/white"
                    android:layout_weight="0.05"
                    android:id="@+id/textSta3" />

                <ImageView
                    android:layout_width="45dp"
                    android:layout_height="50dp"
                    android:src="@drawable/elev"
                    android:gravity="left|center"
                    android:paddingBottom="5dp"
                    />
                <LinearLayout
                    android:orientation="vertical"
                    android:layout_width="100dp"
                    android:layout_height="wrap_content"
                    android:layout_margin="0dp">
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="中央地下通路→中央線快速（上）"
                        android:textSize="12dp"
                        android:paddingTop="5dp"
                        android:id="@+id/place2_1" />
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:text="南口・東南口→埼京線・湘南新宿ライン・中央線快速・中央線特急・中央線各駅停車・山手線"
                        android:textSize="12dp"
                        android:paddingTop="0dp"
                        android:focusable="true"
                        android:id="@+id/place2_2" />
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>
        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="30dp">
            <Button
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="再検索"
                android:background="@color/pink_5"
                android:id="@+id/searchButton2"
                android:layout_gravity="center_horizontal" />
        </LinearLayout>



    </LinearLayout>

</RelativeLayout>